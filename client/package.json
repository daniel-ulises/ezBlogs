{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn sass:build && react-scripts build",
    "sass:watch": "sass --watch --no-source-map ./src/Sass/main.scss:./src/app.min.css",
    "sass:build": "sass --no-source-map --style=compressed ./src/Sass/main.scss:./src/app.min.css && postcss ./src/app.min.css --replace --use autoprefixer"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.13",
    "postcss-cli": "^8.3.1",
    "sass": "^1.32.8",
    "typescript": "^4.2.4"
  }
}
